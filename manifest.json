{
    "manifest_version":2,
    "name":"soundcloud shuffle",
    "short_name":"scshuffle",
    "description":"shuffle soundcloud tracklist properly",
    "homepage_url":"https://github.com/risonakamo/scshuffle",
    "version":"1.0.0",

    "permissions":["https://soundcloud.com/*"],

    "browser_action":{
        "default_popup":"popup.html"
    },

    "icons":{
        "128":"icons/128.png",
        "48":"icons/48.png",
        "16":"icons/16.png"
    }
}