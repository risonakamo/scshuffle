{
    "name":"soundcloud shuffle",
    "short_name":"scshuffle",
    "description":"shuffle soundcloud tracklist properly",
    "homepage_url":"https://github.com/risonakamo/scshuffle",
    "version":"1.1.0",
    "manifest_version":3,
    "author":"risonakamo",

    "permissions": ["scripting"],
    "host_permissions":["https://soundcloud.com/*"],


    "action": {
        "default_title":"Soundcloud Shuffle",
        "default_popup": "popup.html"
    },

    "icons":{
        "128":"icons/128.png",
        "48":"icons/48.png",
        "16":"icons/16.png"
    }
}